'use strict'

const mongoose = require('mongoose')
const Schema = mongoose.Schema

const JointCfg = new Schema({
  manufacturer: String, // 厂商
  url: String, // 拼控地址
  port: Number // 拼控端口
})
mongoose.model('JointCfg', JointCfg)
