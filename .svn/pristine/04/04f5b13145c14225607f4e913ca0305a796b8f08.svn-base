 .btnGroup {
  width: 100%;
  padding: 10px;
  padding-bottom: 0;
  text-align:center;
}
 .btnGroup .btnStyle {
  /* 按钮样式*/
  background-color: rgb(60, 80, 115);
  border-color: rgb(86, 118, 169);
  border-width: 1px;
  border-style: solid;
  color: #fff;
  padding: 0;
  margin: 0;
  width: 16.6666666%;
  height: 24px;
}
 .btnGroup .btnStyle .iconfont {
  font-size: 14px;
}
 .btnGroup .btnStyle:hover {
  /* 按钮样式*/
  background: #4699f9;
}
 .btnGroup .btnStyle[disabled],
 .btnGroup .btnStyle[disabled]:hover {
  /* 禁用按钮样式*/
  background: #535777;
  color: #cacaca;
}
